{
  "browser": {
    "executablePath": "/snap/bin/chromium"
  },
  "models": {
    "providers": {
      "litellm": {
        "baseUrl": "http://{LLM-IP:PORT}/v1",
        "apiKey": "{API-KEY}",
        "api": "openai-completions",
        "authHeader": false,
        "headers": {
          "x-litellm-api-key": "{API-KEY}"
        },
        "models": [
          {
            "id": "gemma-3-27b-it",
            "name": "Gemma 3 27B IT",
            "reasoning": false,
            "input": ["text"],
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "litellm/gemma-3-27b-it"
      },
      "models": {
        "litellm/gemma-3-27b-it": {
          "alias": "gemma"
        }
      },
      "workspace": "/home/intern/.openclaw/workspace",
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "tools": {
    "byProvider": {
      "litellm/gemma-3-27b-it": {
        "deny": ["*"]
      }
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    "mattermost": {
      "enabled": true,
      "baseUrl": "https://{YOUR-MATTERMOST-URL}",
      "botToken": "{BOT-TOKEN}",
      "dmPolicy": "pairing",
      "chatmode": "onchar",
      "oncharPrefixes": [">"]
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "{AUTO-GENERATED-TOKEN}"
    }
  },
  "skills": {
    "install": {
      "nodeManager": "npm"
    }
  },
  "plugins": {
    "entries": {
      "mattermost": {
        "enabled": true
      }
    }
  }
}
